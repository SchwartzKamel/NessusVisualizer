{% extends 'layout.jinja2' %}

{% block title %}
  Scan Results
{% endblock %}

{% block styles %}
<link rel="stylesheet" href="{{ url_for('static', filename='css/style.css') }}" rel="stylesheet" type="text/css">
{% endblock %}

{% block content %}
  <h1>Scan Results</h1>
    <table class="scan-results">
      <tbody>
        {% for result in scan_data %}
          <tr>
            <td>{{ result }}</td>
          </tr>
        {% endfor %}
      </tbody>
    </table>
{% endblock %}